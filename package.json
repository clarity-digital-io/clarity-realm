{
	"name": "clarity-realm-listener",
	"version": "1.0.0",
	"description": "Realm Listener",
	"license": "MIT",
	"scripts": {
		"start": "npm run prod",
		"build": "npm-run-all clean transpile",
		"server": "node ./dist-server/index.js",
		"dev": "NODE_ENV=development npm-run-all build server",
		"prod": "NODE_ENV=production npm-run-all build server",
		"transpile": "babel ./server --out-dir dist-server",
		"clean": "rimraf dist-server",
		"watch:dev": "nodemon"
	},
	"nodemonConfig": {
		"exec": "npm run dev",
		"watch": [
			"server/*"
		]
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-transform-runtime"
		]
	},
	"dependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.4",
		"@babel/plugin-transform-runtime": "^7.10.1",
		"@babel/preset-env": "^7.8.4",
		"auth0": "^2.25.1",
		"axios": "^0.19.2",
		"body-parser": "^1.19.0",
		"bull": "^3.14.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.1.0",
		"npm-run-all": "^4.1.5",
		"realm": "^6.0.1",
		"rimraf": "^3.0.2"
	}
}
